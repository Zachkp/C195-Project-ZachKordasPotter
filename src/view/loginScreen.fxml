<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <Label fx:id="mainLabel" layoutX="290.0" layoutY="14.0" text="Login Screen">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="326.0" layoutY="138.0" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="326.0" layoutY="180.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="326.0" layoutY="218.0" mnemonicParsing="false" onAction="#loginButtonPressed" prefHeight="25.0" prefWidth="149.0" text="Login" />
      <Label fx:id="locationLabel" layoutX="222.0" layoutY="353.0" text="Location">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="languageLabel" layoutX="510.0" layoutY="353.0" text="Language">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
